{
  "name": "frooky-agent",
  "version": "0.0.1",
  "description": "Frooky frida agent.",
  "private": true,
  "type": "module",
  "scripts": {
    "build:prod:android": "node build.js -p android -c --type-check none",
    "build:prod:ios": "node build.js -p ios -c --type-check none",
    "build:dev:android": "node build.js -p android --type-check none",
    "build:dev:ios": "node build.js -p ios --type-check none",
    "build:watch:android": "node build.js -p android -t frida -w --type-check none",
    "build:watch:ios": "node build.js -p ios -w -t frida --type-check none",
    "build:test:android": "frida-compile --type-check none tests/android/agent-runner.ts -o dist/agent-test-android.js",
    "build:test:ios": "frida-compile --type-check none tests/ios/agent-runner.ts -o dist/agent-test-ios.js",
    "test:android": "npm run build:test:android && node run-tests.js -p android",
    "test:ios": "npm run build:test:ios && node run-tests.js -p ios"
  },
  "devDependencies": {
    "@types/frida-gum": "^19.0.1",
    "@types/node": "^24.10.9",
    "chokidar": "^5.0.0",
    "frida-compile": "^19.0.4",
    "frida-java-bridge": "^7.0.11",
    "frida-objc-bridge": "^8.0.5",
    "frida-swift-bridge": "^3.0.2",
    "minimist": "^1.2.8"
  },
    "overrides": {
    "@isaacs/brace-expansion": "^5.0.1"
  }
}
